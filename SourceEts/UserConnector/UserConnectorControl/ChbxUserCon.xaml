<UserControl x:Class="SourceEts.UserConnector.UserConnectorControl.ChbxUserCon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Name="UserControl" Height="28" MinWidth="450" HorizontalAlignment="Stretch" >

    <Grid x:Name="LayoutRoot">
        <Grid>
            <CheckBox Margin="179,2,0,0" HorizontalAlignment="Left" Width="21" Height="24" VerticalAlignment="Top" Name="Chbx"/>
            <TextBlock Cursor="Help" Foreground="White" Height="16" HorizontalAlignment="Left" Name="Txbl" VerticalAlignment="Top" Width="164" Margin="15,6,0,0" >
                <TextBlock.ToolTip>
                    <ToolTip>
                        <DockPanel Width="250">
                            <TextBlock TextWrapping="Wrap" Name="TxblToolTip"></TextBlock>
                        </DockPanel>
                    </ToolTip>
                </TextBlock.ToolTip> 
			</TextBlock>
        </Grid>
    </Grid>
</UserControl>
